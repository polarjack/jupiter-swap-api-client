# Jupiter API Configuration
# Copy this file to .env and fill in your values

# Jupiter API base URL (optional, defaults to https://api.jup.ag/swap/v1)
API_BASE_URL=https://api.jup.ag/swap/v1

# Jupiter API key for authenticated endpoints (optional)
JUPITER_API_KEY=your-api-key-here

# Solana RPC URL (optional, defaults to https://api.mainnet-beta.solana.com)
SOLANA_RPC_URL=https://api.mainnet-beta.solana.com
